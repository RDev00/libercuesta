---
export interface Props {
	title?: String
}

const { title = 'N/I' } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title> { title } </title>
</head>
<body>
	<slot /> <!-- Content of web -->
</body>
</html>